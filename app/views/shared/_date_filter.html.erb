<div class="container">
  <div class="form-wrapper" style="width: 400px; margin: 0 auto;">
    <h2> Time Range</h2>

    <%= simple_form_for :search, method: "get" do |f| %>
      <%= f.input :starts_at, label: "Pick Dates", as: :string, required: false, input_html: {class: "datepicker"} %>
      <%= f.hidden_field :ends_at, as: :string, required: false, input_html: {class: "datepicker"} %>
      <%= f.button :submit, "Go", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
