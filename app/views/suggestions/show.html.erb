<div class="banner-dashboard">
  <canvas class="background" style="position: absolute; top: 0; left: 0;"></canvas>

</div>

<div class= "container-structure-show">


  <%= image_tag current_user.avatar, class: "avatar-large m-3" %>
  <div class= "dates">
    <i class="fab fa-youtube"></i>
    <h1><%= Date.today.strftime("%A, %b %d") %></h1>
  </div>

<% if current_user.youtube_accounts.count == 0 %>
  <a href=<%= @authorization_url %>>Please add a Youtube Account</a>
</div>
<% else %>
  <h1>Hello <strong><%= current_user.full_name %></strong> Check out this channel</h1>


  <div class="dashboard-frame row">
    <div class="col-md-4">
      <div class="user-card">

        <%= image_tag current_user.avatar, class: "avatar-large m-3" %>
        <div class= "dates">
          <i class="fab fa-youtube"></i>
          <h1><%= Date.today.strftime("%A, %b %d") %></h1>
        </div>
        <p> TEXT -labore et </p>
      </div>
    </div>
    <div class="col-md-8">
      <div class="suggestion-show-videos row">
        <%# .each do || %>
          <div class="col-md-4 col-sm-6 card"></div>
            <div class="card-group">
